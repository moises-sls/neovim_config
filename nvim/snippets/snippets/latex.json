    "frame": {
        "prefix": "BFR",
        "body": [
            "\\begin{frame}",
            "    \\frametitle{${1:<title>}}",
            "",
            "    ${2}",
            "",
            "\\end{frame}"
        ],
        "description": "frame"
    },
    "figure": {
        "prefix": "BFI",
        "body": [
            "\\begin{figure}[${1:htbp}]",
            "    \\centering",
            "    ${2}",
            "    \\caption{${3:<caption>}}\\label{${4:<label>}}",
            "\\end{figure}"
        ],
        "description": "figure"
    },
    "subfigure": {
        "prefix": "BSFI",
        "body": [
            "\\begin{figure}[!ht]",
            "    \\centering",
            "    \\begin{subfigure}[t]{0.4\\textwidth}",
            "        \\centering",
            "        \\includegraphics[width=\\textwidth]{${1}}",
            "        \\caption{${2}}",
            "    \\end{subfigure}%",
            "    ~",
            "    \\begin{subfigure}[t]{0.4\\textwidth}",
            "        \\centering",
            "        \\includegraphics[width=\\textwidth]{${3}}",
            "        \\caption{${4}}",
            "    \\end{subfigure}%",
            "    \\caption{${5}}",
            "\\end{figure}"
        ],
        "description": "subfigure"
    },
    "table": {
        "prefix": "BTA",
        "body": [
            "\\begin{table}[${1:htbp}]",
            "    \\centering    ",
            "    \\begin{tabular}{${2:<columns>}}",
            "        ${3}",
            "    \\end{tabular}",
            "    \\caption{${4:<caption>}}    ",
            "    \\label{${5:<label>}}",
            "\\end{table}"
        ],
        "description": "table"
    },
